{
  "analytics": {
    "buddyapp": {
      "providerPlugin": "awscloudformation",
      "service": "Pinpoint"
    }
  },
  "api": {
    "buddyappv2": {
      "dependsOn": [
        {
          "attributes": [
            "UserPoolId"
          ],
          "category": "auth",
          "resourceName": "buddyappv2auth"
        }
      ],
      "output": {
        "authConfig": {
          "additionalAuthenticationProviders": [
            {
              "authenticationType": "AWS_IAM"
            },
            {
              "authenticationType": "AMAZON_COGNITO_USER_POOLS",
              "userPoolConfig": {
                "userPoolId": "authbuddyappv2auth"
              }
            }
          ],
          "defaultAuthentication": {
            "apiKeyConfig": {
              "apiKeyExpirationDays": 30,
              "description": "api key description"
            },
            "authenticationType": "API_KEY"
          }
        }
      },
      "providerPlugin": "awscloudformation",
      "service": "AppSync"
    }
  },
  "auth": {
    "buddyappv2auth": {
      "customAuth": false,
      "dependsOn": [
        {
          "attributes": [
            "Arn",
            "Name"
          ],
          "category": "function",
          "resourceName": "buddyappv2authPostConfirmation",
          "triggerProvider": "Cognito"
        }
      ],
      "frontendAuthConfig": {
        "mfaConfiguration": "OPTIONAL",
        "mfaTypes": [
          "SMS"
        ],
        "passwordProtectionSettings": {
          "passwordPolicyCharacters": [],
          "passwordPolicyMinLength": 8
        },
        "signupAttributes": [
          "EMAIL",
          "FAMILY_NAME",
          "GIVEN_NAME",
          "PHONE_NUMBER",
          "PREFERRED_USERNAME"
        ],
        "socialProviders": [],
        "usernameAttributes": [
          "EMAIL"
        ],
        "verificationMechanisms": [
          "EMAIL"
        ]
      },
      "providerPlugin": "awscloudformation",
      "service": "Cognito"
    },
    "userPoolGroups": {
      "dependsOn": [
        {
          "attributes": [
            "UserPoolId",
            "AppClientIDWeb",
            "AppClientID",
            "IdentityPoolId"
          ],
          "category": "auth",
          "resourceName": "buddyappv2auth"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "Cognito-UserPool-Groups"
    }
  },
  "function": {
    "buddyappv2authPostConfirmation": {
      "build": true,
      "dependsOn": [
        {
          "attributes": [
            "GraphQLAPIIdOutput",
            "GraphQLAPIEndpointOutput",
            "GraphQLAPIKeyOutput"
          ],
          "category": "api",
          "resourceName": "buddyappv2"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  },
  "notifications": {
    "buddyapp": {
      "channels": [
        "APNS"
      ],
      "service": "Pinpoint"
    }
  },
  "parameters": {
    "AMPLIFY_analytics_Pinpoint_Id": {
      "usedBy": [
        {
          "category": "analytics",
          "resourceName": "Pinpoint"
        }
      ]
    },
    "AMPLIFY_analytics_Pinpoint_Name": {
      "usedBy": [
        {
          "category": "analytics",
          "resourceName": "Pinpoint"
        }
      ]
    },
    "AMPLIFY_analytics_Pinpoint_Region": {
      "usedBy": [
        {
          "category": "analytics",
          "resourceName": "Pinpoint"
        }
      ]
    },
    "AMPLIFY_function_buddyappv2authPostConfirmation_GROUP": {
      "usedBy": [
        {
          "category": "function",
          "resourceName": "buddyappv2authPostConfirmation"
        }
      ]
    },
    "AMPLIFY_function_buddyappv2authPostConfirmation_deploymentBucketName": {
      "usedBy": [
        {
          "category": "function",
          "resourceName": "buddyappv2authPostConfirmation"
        }
      ]
    },
    "AMPLIFY_function_buddyappv2authPostConfirmation_s3Key": {
      "usedBy": [
        {
          "category": "function",
          "resourceName": "buddyappv2authPostConfirmation"
        }
      ]
    },
    "AMPLIFY_notifications_Pinpoint_Id": {
      "usedBy": [
        {
          "category": "notifications",
          "resourceName": "Pinpoint"
        }
      ]
    },
    "AMPLIFY_notifications_Pinpoint_Name": {
      "usedBy": [
        {
          "category": "notifications",
          "resourceName": "Pinpoint"
        }
      ]
    },
    "AMPLIFY_notifications_Pinpoint_Region": {
      "usedBy": [
        {
          "category": "notifications",
          "resourceName": "Pinpoint"
        }
      ]
    }
  },
  "storage": {
    "s3buddyappv2media": {
      "dependsOn": [],
      "providerPlugin": "awscloudformation",
      "service": "S3"
    }
  }
}